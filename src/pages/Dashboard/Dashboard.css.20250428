.dash-chip-container {
  display: flex;
  align-items: center;
  flex-wrap: wrap;
  gap: 8px;
}

.dash-chip {
  background-color: #f4836c;
  padding: 4px 8px;
  border-radius: 16px;
  display: flex;
  align-items: center;
  font-size: 12px;
  color: #fff;
  letter-spacing: 0.3px;
}

.dash-chip-close {
  margin-left: 8px;
  cursor: pointer;
  font-size: 10px;
  color: #fff;
}
.lead-summary-data {
  width: 120px;
}
.lead-summary-data .dropdown-container {
  min-width: 120px !important;
}
.lead-summary-data,
.selected-role {
  margin-top: 0;
}
