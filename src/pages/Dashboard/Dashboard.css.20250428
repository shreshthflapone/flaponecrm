/* table css */
.ld-dash .table-wrapper {
  /* max-height: 360px; */
  overflow: auto;
  border: 1px solid #ddd;
  border-radius: 5px;
  border-bottom: none;
}

.ld-dash table {
  width: 100%;
  border-collapse: collapse;
  text-align: left;
}

.ld-dash thead tr {
  position: sticky;
  top: 0;
  background-color: #f7f7f7;
  z-index: 1;
}

.ld-dash th,
.ld-dash td {
  padding: 12px;
  white-space: nowrap;
}

.ld-dash tr:last-child {
  border-bottom: 1px solid #ddd;
}

.ld-dash tbody tr:nth-child(even),
.ld-dash tbody tr:nth-child(even) td:first-child,
.ld-dash tbody tr:nth-child(even) td:nth-child(2) {
  background-color: #dfdcdc;
}

.ld-dash th:first-child,
.ld-dash td:first-child {
  position: sticky;
  left: 0;
  background-color: #fff;
  z-index: 1;
}

.ld-dash th:nth-child(2),
.ld-dash td:nth-child(2) {
  position: sticky;
  left: 140px;
  background-color: #fff;
  z-index: 1;
}
.ld-dash th:first-child,
.ld-dash th:nth-child(2) {
  background-color: #f0f0f0;
}
