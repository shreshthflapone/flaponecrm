/* card slider  */

.ld-slider-container {
  position: relative;
  width: 100%;
  overflow: hidden;
  padding: 12px 0;
}

.ld-slider {
  display: flex;
  gap: 10px;
  overflow-x: auto;
  scroll-behavior: smooth;
  scrollbar-width: none;
  padding-bottom: 8px;
}

.ld-slider::-webkit-scrollbar {
  display: none;
}

.ld-card {
  flex: 0 0 12%;
  border: 1px solid #ddd;
  border-radius: 8px;
  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
  text-align: center;
  min-width: 150px;
  transition: transform 0.3s ease;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}

.ld-card h3 {
  font-size: 16px;
  color: #333;
  margin-bottom: 10px;
}

.ld-value {
  font-size: 18px;
  font-weight: bold;
  color: #333;
}
.ld-slider-container .progress-segment {
  height: 8px !important;
}

.ld-card h3 {
  font-size: 16px;
  color: #333;
  margin-bottom: 10px;
}

.ld-value {
  font-size: 18px;
  font-weight: bold;
  color: #333;
}

.ld-percentage {
  font-size: 14px;
}

.ld-negative {
  color: #d9534f;
}

.ld-positive {
  color: #5cb85c;
}

.ld-slide-btn {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  background-color: #f1f1f1;
  border: none;
  border-radius: 50%;
  cursor: pointer;
  padding: 8px;
  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.2);
  z-index: 1;
}

.ld-slide-left {
  left: 10px;
}

.ld-slide-right {
  right: 10px;
}

.ld-slide-btn:hover {
  background-color: #e0e0e0;
}

.ld-slider-container .progress-segment:first-child {
  border-bottom-left-radius: 4px;
}
.ld-slider-container .progress-segment:last-child {
  border-bottom-right-radius: 4px;
}

