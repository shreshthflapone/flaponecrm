.compare-toggle {
  position: relative;
  width: 24px;
  height: 12px;
  background-color: #ff3b3b;
  border-radius: 50px;
  transition: background-color 0.3s ease;
}
.compare-toggle-circle {
  position: absolute;
  top: 1.5px;
  left: 2px;
  width: 10px;
  height: 10px;
  background-color: white;
  border-radius: 50%;
  transition: left 0.3s ease;
}
.compare-toggle-on {
  background-color: #088408;
}
.compare-toggle-circle-on {
  left: 12px;
}
