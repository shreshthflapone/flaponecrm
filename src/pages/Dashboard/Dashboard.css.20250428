@media (max-width: 360px) {
  /* .recharts-wrapper {
    max-width: 240px !important;
    margin: 0 auto;
  } */
}
@media (max-width: 768px) {
  .row,
  .st-das-box,
  .batch-chart {
    flex-direction: column;
  }
  .st-dash-card {
    flex-direction: row !important;
  }
  .br1 {
    border-right: none;
  }
  .card,
  .chart {
    min-width: 100%;
  }

  .ld-st-gr .recharts-responsive-container {
    max-width: 100% !important;
  }
  .ld-card {
    flex: 0 0 48% !important;
    max-width: 48%;
    min-width: auto !important;
  }
  .st-crd .card {
    flex: 0 0 100%;
    min-width: 100%;
  }
  .crm-dashboard.card {
    padding: 8px !important;
  }
  .ver .recharts-legend-wrapper {
    top: auto !important;
    bottom: -8px !important;
    font-size: 13px !important;
  }
  .ver .recharts-default-legend {
    display: flex;
    align-items: center;
    justify-content: center;
    flex-wrap: wrap;
  }

  .ver .recharts-legend-item {
    margin-bottom: 8px !important;
  }
  .sales-dashboard .recharts-legend-item {
    margin-bottom: 4px !important;
  }
  .ver .recharts-legend-wrapper {
    left: 0 !important;
  }
  .sales-sum-head{
    padding-left: 12px;
    padding-right: 12px;
  }
  .sales-sum .chart{
    padding-left: 0;
    padding-right: 0;
  }
  .recharts-legend-wrapper{
    width: auto !important;
  }

  .sales-sum .recharts-surface{
    /* height: 264px !important; */
  }
}
@media (min-width: 769px) and (max-width: 1280px) {
  .ld-st-gr .recharts-responsive-container {
    max-width: 80% !important;
  }
  .dashboard-container .card {
    min-width: 264px;
  }
}
